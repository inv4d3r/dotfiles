URxvt.color5:     #FF79C6
URxvt.color13:    #FF92D0
URxvt.color6:     #8BE9FD
URxvt.color14:    #9AEDFE
URxvt.color7:     #BFBFBF
URxvt.color15:    #E6E6E6

URxvt*scrollBar : false

URxvt*depth: 32
!URxvt*font: xft:inconsolata:size=10:style=Regular:antialias=true
URxvt*font: xft:inconsolata for powerline:size=10:style=Regular:antialias=true
!URxvt.font: xft:ProggyTinyTT:size=12
!URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10:style=Book:antialias=true
!URxvt.font: xft:dina:size=8:style=Regular

! Clickable links
URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/qutebrowser
URxvt.colorUrl : #4682b4
! Yankable links
!URxvt.perl-ext: default, url-select
!URxvt.url-select-launcher: /usr/bin/firefox -new-tab
!URxvt.url-select.underline: true
!URxvt.keysym.M-u: perl:url-select:select_next
!URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
