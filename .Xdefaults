URxvt.geometry: 130x35

URxvt.scrollBar : false
URxvt.internalBorder: 10

URxvt.depth: 32
URxvt.background: rgba:0000/0000/0200/b000
URxvt.foreground : gray
URxvt.font: xft:inconsolata:size=10:style=Regular:antialias=true
!URxvt.font: xft:ProggyTinyTT:size=12
!URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10:style=Book:antialias=true
!URxvt.font: xft:dina:size=8:style=Regular

! Clickable links
URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
!URxvt.url-launcher: /usr/bin/firefox
URxvt.url-launcher: /usr/bin/luakit
URxvt.colorUrl : #4682b4
! Yankable links
!URxvt.perl-ext: default, url-select
!URxvt.url-select-launcher: /usr/bin/firefox -new-tab
!URxvt.url-select.underline: true
!URxvt.keysym.M-u: perl:url-select:select_next
!URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
