! URxvt gruvbox dark soft
URxvt.background: [70]#32302f
URxvt.foreground : #928374

! ! Black + DarkGrey
URxvt*color0: #282828
URxvt*color8: #928374
! DarkRed + Red
URxvt*color1: #cc241d
URxvt*color9: #fb4934
! DarkGreen + Green
URxvt*color2: #98971a
URxvt*color10: #b8bb26
! DarkYellow + Yellow
URxvt*color3: #d79921
URxvt*color11: #fabd2f
! DarkBlue + Blue
URxvt*color4: #458588
URxvt*color12: #83a598
! DarkMagenta + Magenta
URxvt*color5: #b16286
URxvt*color13: #d3869b
! DarkCyan + Cyan
URxvt*color6: #689d6a
URxvt*color14: #8ec07c
! LightGrey + White
URxvt*color7: #a89984
URxvt*color15: #ebdbb2

URxvt*scrollBar : false

URxvt*depth: 32
URxvt*font: xft:inconsolata:size=10:style=Regular:antialias=true
!URxvt.font: xft:ProggyTinyTT:size=12
!URxvt.font: xft:DejaVu Sans Mono for Powerline:size=10:style=Book:antialias=true
!URxvt.font: xft:dina:size=8:style=Regular

! Clickable links
URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/qutebrowser
URxvt.colorUrl : #4682b4
! Yankable links
!URxvt.perl-ext: default, url-select
!URxvt.url-select-launcher: /usr/bin/firefox -new-tab
!URxvt.url-select.underline: true
!URxvt.keysym.M-u: perl:url-select:select_next
!URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
